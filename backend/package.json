{
  "name": "gymstack-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "build": "prisma generate --schema=./prisma/schema.prisma && nest build",
    "predeploy": "npm run lint && npm run typecheck && npm run build",
    "start": "npm run start:prod",
    "start:prod": "bash scripts/start-prod.sh",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:tables": "node scripts/db-check.js",
    "test": "jest",
    "db:seed": "prisma db seed --schema=./prisma/schema.prisma",
    "db:setup": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma:validate": "prisma validate --schema=./prisma/schema.prisma",
    "ensure:qa-user": "node -e \"require('fs').rmSync('.tmp/qa-script',{recursive:true,force:true})\" && tsc --module commonjs --target es2020 --esModuleInterop --skipLibCheck --moduleResolution node --outDir .tmp/qa-script scripts/ensure-qa-user.ts src/auth/password-hasher.ts && node .tmp/qa-script/scripts/ensure-qa-user.js"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@nestjs/cli": "^10.4.4",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^5.18.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "libphonenumber-js": "^1.11.1",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^16.10.0",
    "typescript": "^5.4.2",
    "validator": "^13.14.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.4",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "prisma": "^5.18.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5"
  },
  "prisma": {
    "seed": "node prisma/seed.ts"
  }
}
