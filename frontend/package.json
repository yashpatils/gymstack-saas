{
  "name": "gymstack-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "preflight": "npm run lint && npm run typecheck && npm run build",
    "predeploy": "npm run lint && npm run typecheck && npm run build",
    "smoke": "playwright test tests/smoke.spec.ts",
    "smoke:headed": "playwright test tests/smoke.spec.ts --headed",
    "qa:full": "playwright test tests/qa-full.spec.ts"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "2.12.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "20.12.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "jsdom": "^24.1.0",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.6",
    "typescript": "5.4.5",
    "vitest": "^2.0.5"
  }
}
